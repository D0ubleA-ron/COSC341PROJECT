<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EFE9E7"
    tools:context=".Matches_Screen">

    <LinearLayout
        android:id="@+id/stevenLayout"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="70dp"
        tools:visibility="visible">

        <ImageButton
            android:id="@+id/imageButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#EFE9E7"
            android:onClick="ButtonClick"
            android:tag="0"
            app:srcCompat="@drawable/ellipse_1" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/nunito"
            android:text="Steven"
            android:textAlignment="center"
            android:textColor="#111344"
            android:textSize="16sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/nunito"
            android:text="(Help Baking Bread)"
            android:textAlignment="center"
            android:textColor="#111344"
            android:textStyle="bold|italic" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/marleyLayout"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="225dp"
        tools:layout_editor_absoluteY="70dp"
        tools:visibility="visible">

        <ImageButton
            android:id="@+id/imageButton4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#EFE9E7"
            android:onClick="ButtonClick"
            android:tag="1"
            app:srcCompat="@drawable/ellipse_2_ek4" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/nunito"
            android:text="Marley"
            android:textColor="#111344"
            android:textSize="16sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/nunito"
            android:text="(Wants Life Advice)"
            android:textColor="#111344"
            android:textStyle="bold|italic" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/samLayout"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="250dp"
        tools:visibility="visible">

        <ImageButton
            android:id="@+id/imageButton2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#EFE9E7"
            android:onClick="ButtonClick"
            android:tag="2"
            app:srcCompat="@drawable/ellipse_2_ek1" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/nunito"
            android:text="Sam"
            android:textColor="#111344"
            android:textSize="16sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/nunito"
            android:text="(Teach Overwatch 2)"
            android:textColor="#111344"
            android:textStyle="bold|italic" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/paulLayout"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="225dp"
        tools:layout_editor_absoluteY="250dp"
        tools:visibility="visible">

        <ImageButton
            android:id="@+id/imageButton5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#EFE9E7"
            android:onClick="ButtonClick"
            android:tag="3"
            app:srcCompat="@drawable/ellipse_2_ek3" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/nunito"
            android:text="Paul"
            android:textColor="#111344"
            android:textSize="16sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/nunito"
            android:text="(Help Install TV)"
            android:textColor="#111344"
            android:textStyle="bold|italic" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/jessLayout"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="430dp"
        tools:visibility="visible">

        <ImageButton
            android:id="@+id/imageButton3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#EFE9E7"
            android:onClick="ButtonClick"
            android:tag="4"
            app:srcCompat="@drawable/ellipse_2" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/nunito"
            android:text="Jess"
            android:textColor="#111344"
            android:textSize="16sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/nunito"
            android:text="(Fix Your Computer)"
            android:textColor="#111344"
            android:textStyle="bold|italic" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/alexLayout"
        android:layout_width="180dp"
        android:layout_height="180dp"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="225dp"
        tools:layout_editor_absoluteY="430dp"
        tools:visibility="visible">

        <ImageButton
            android:id="@+id/imageButton6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#EFE9E7"
            android:onClick="ButtonClick"
            android:tag="5"
            app:srcCompat="@drawable/ellipse_2_ek2" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/nunito"
            android:text="Alex"
            android:textColor="#111344"
            android:textSize="16sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/nunito"
            android:text="(Board Game Partner)"
            android:textColor="#111344"
            android:textStyle="bold|italic" />
    </LinearLayout>

    <ImageButton
        android:id="@+id/homeButton4"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:background="#00FFFFFF"
        android:onClick="NavBarClick"
        android:scaleType="fitCenter"
        android:scaleX=".60"
        android:scaleY=".60"
        android:tag="home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/home" />

    <ImageButton
        android:id="@+id/messagesButton4"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:layout_marginStart="9dp"
        android:background="#00FFFFFF"
        android:onClick="NavBarClick"
        android:scaleType="fitCenter"
        android:scaleX=".60"
        android:scaleY=".60"
        android:tag="messages"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/homeButton4"
        app:srcCompat="@drawable/messages" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:layout_marginEnd="9dp"
        android:background="#00FFFFFF"
        android:scaleType="fitCenter"
        android:scaleX=".6"
        android:scaleY=".6"
        android:tag="matches"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/settingsButton4"
        app:srcCompat="@drawable/matches_fill" />

    <ImageButton
        android:id="@+id/settingsButton4"
        android:layout_width="96dp"
        android:layout_height="96dp"
        android:background="#00FFFFFF"
        android:onClick="NavBarClick"
        android:scaleType="fitCenter"
        android:scaleX=".60"
        android:scaleY=".60"
        android:tag="settings"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/settings" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nunito"
        android:text="Potential Matches"
        android:textColor="#111344"
        android:textSize="32sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginTop="44dp"
        android:background="@android:color/darker_gray"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="0dp" />

</androidx.constraintlayout.widget.ConstraintLayout>